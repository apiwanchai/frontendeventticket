{
  "extends": ["@nuxtjs", "plugin:nuxt/recommended", "prettier"],
  // add your custom rules here
  "rules": {
    "no-undef": 0,
    "no-console": 1,
    "vue/no-multiple-template-root": 0,
    "vue/multi-word-component-names": 0
